{
  "name": "teamto-doapp",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start --watch",
    "start:dev": "export NODE_ENV=development && nest start",
    "start:dev:watch": "export NODE_ENV=development && nest start --watch",
    "start:prod": "export NODE_ENV=production && npm run build && node dist/main",
    "db:sync": "npx sequelize-cli db:seed:all --config .database/config/config.json --seeders-path .database/seeders/",
    "commit": "git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pkg": "npm run build && pkg . --target linux"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "7.6.5",
    "@nestjs/core": "7.4.4",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.4",
    "@nestjs/platform-fastify": "7.4.4",
    "@nestjs/platform-socket.io": "7.6.4",
    "@nestjs/schedule": "0.4.1",
    "@nestjs/sequelize": "0.1.0",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/websockets": "7.6.4",
    "argon2": "0.27.0",
    "async-redis": "1.1.7",
    "axios": "0.21.1",
    "chalk": "4.1.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "fastify": "3.6.0",
    "fastify-helmet": "5.0.3",
    "fastify-multipart": "3.2.0",
    "fastify-swagger": "^4.9.0",
    "file-type": "16.0.1",
    "load-env-var": "0.4.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "nestjs-redis": "1.3.3",
    "node-forge": "0.10.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.3.3",
    "pg-native": "3.0.0",
    "pino": "6.7.0",
    "pino-multi-stream": "5.1.1",
    "pino-pretty": "4.3.0",
    "promise-socket": "7.0.0",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "sequelize": "5.21.11",
    "sequelize-typescript": "1.1.0",
    "socket.io": "3.0.4",
