    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.0.1",
    "@types/async-redis": "1.1.1",
    "@types/cron": "1.7.2",
    "@types/express": "4.17.3",
    "@types/node": "13.9.1",
    "@types/node-forge": "0.9.5",
    "@types/passport-jwt": "3.0.3",
    "@types/pino": "6.3.4",
    "@types/pino-multi-stream": "5.1.0",
    "@types/redis": "2.8.28",
    "@types/sequelize": "4.28.9",
