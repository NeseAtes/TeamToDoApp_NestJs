    "pkg": "4.4.9",
    "prettier": "1.19.1",
    "sequelize-cli": "6.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint -s"
    }
  },
  "pkg": {
    "assets": [
      "keys/**/*",
      ".database/**/*",
      ".env"
    ]
  },
  "bin": "dist/main.js"
}
